<h1>Our Bikes</h1>
  <div class="container">
    <div class="row">
      <% cache(cache_key_for_products) do %>
        <%= render @products %>
      <% end %>
    </div>
    <div class="new-product row">
      <div class="col-md-2 col-xs-12">
        <% if signed_in? && current_user.admin? %>
        <%= link_to 'New Product', new_product_path, :class => "btn btn-default" %>
        <% end %>
      </div>
    </div>
  </div>
